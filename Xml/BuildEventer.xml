<?xml version="1.0" encoding="utf-8" ?>
<Object>

  <Actions>
    <Copy>
      <Destination>ABC\DEF</Destination>
      <Sources>
        <Source>$(ProjectName)\$(Platform)$(Configuration)\*.dll</Source>
        <Source>$(ProjectName)\$(Platform)$(Configuration)\*.cpp</Source>
      </Sources>
    </Copy>
  </Actions>

  <Parameters>
    <Parameter>$(ProjectName)</Parameter>
    <Parameter>$(Platform)$(Configuration)</Parameter>
  </Parameters>

  <Filters>
    <Filter>*.dll</Filter>
    <Filter>*.lib</Filter>
    <Filter>*.cpp</Filter>
  </Filters>

  <BranchExplorers>
    <BranchExplorer>
      <Parent>D:\</Parent>
      <Sub>$(ProjectName)</Sub>
    </BranchExplorer>
    
    <BranchExplorer>
      <Parent>D:\$(ProjectName)</Parent>
      <Sub>$(Platform)</Sub>
    </BranchExplorer>
  </BranchExplorers>
  
</Object>